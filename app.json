{
  "expo": {
    "name": "Simple-Contact",
    "slug": "Simple-Contact",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#d9d9d9"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.auliakbarh.simplecontact"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#d9d9d9"
      },
      "package": "com.auliakbarh.simplecontact"
    },
    "web": {
      "bundler": "webpack",
      "favicon": "./assets/favicon.png"
    }
  }
}
